<!--Header-->
<div class="header">
    <div class="banner" role="banner">
        <div class="title-wrapper">
            <img src="/images/logo.png" alt="The Brown Noser logo" class="logo-image">
            <div class="title-text"><a href="/" style="text-decoration: none;color:black">THE BROWN NOSER</a></div>
        </div>
        <div class="extras-wrapper">
            <div class="centering-wrapper">
            <div class="extras-options">
                <a href="/about" class="extras-text" tabindex="2">About</a>
                <a href="/archive" class="extras-text" tabindex="3">Archive</a>
                <a href="/join" class="extras-text" tabindex="4">Join Us</a>
            </div>
            <div class="search" role="search">
                <form action="/search">
                    <input type="text" placeholder="Search..." name="search" class="search-input search-light-grey" tabindex="5">
                </form>
            </div>
            </div>
        </div>
    </div>
    <hr>
    <div class="navbar" role="navigation">
        <ul class="navlist">
            <li class="navitem"><a href="/category/15" class="hover category">BUSINESS</a></li>
            <li class="navitem"><a href="/category/14" class="hover category">CAMPUS LIFE</a></li>
            <li class="navitem"><a href="/category/50" class="hover category">CINEMA CORNER</a></li>
            <li class="navitem"><a href="/category/17" class="hover category">OFF CAMPUS</a></li>
            <li class="navitem"><a href="/category/45" class="hover category">NEWS IN PICTURES</a></li>
            <li class="navitem"><a href="/category/18" class="hover category">OPINION</a></li>
            <li class="navitem"><a href="/category/48" class="hover category">POLITICS</a></li>
            <li class="navitem"><a href="/category/19" class="hover category">SCIENCE &amp; TECHNOLOGY</a></li>
            <li class="navitem"><a href="/category/20" class="hover category">SPORTS</a></li>
        </ul>
    </div>
    <hr>
</div>

<div class="one-col-page-wrapper" role="main">
    <!--Breadcrumbs-->
    {{!-- <div class="breadcrumbs-wrapper brown bread-margin">
        Previous / <span class="underline">Archive</span>
    </div> --}}
    <!--Sub-banner-->
    <div class="sub-banner brown">
        ARCHIVE
    </div>
    <hr>
    <!--Timeline Section-->
    <div class="timeline-wrapper">
        <ul class="navlist">
            {{#timelineYears}}
            <li class="timeline-button"><a href= {{ year }} class="hover" style={{ yearStyle }}>{{ year }}</a></li>
            {{/timelineYears}}
        </ul>
    </div>
    <!--Bottom Content Section-->
    <div class="bottom-wrapper">
        <div class="bottom-title">ISSUES: {{year}}</div>
        <hr>
        <div class="issues-wrapper">
            <div class="issue-lists-wrapper">
                <ul class="issue-list">
                    {{#archiveList1}}
                    <li class="archive-item"><a href={{ issueLink }} style="color:#573B03">{{ issueDate }}</a></li>
                    {{/archiveList1}}
                </ul>
                <ul class="issue-list">
                    {{#archiveList2}}
                    <li class="archive-item"><a href={{ issueLink }} style="color:#573B03">{{ issueDate }}</a></li>
                    {{/archiveList2}}
                </ul>
            </div>
            <div class="archive-image-wrapper">
                {{#if hasIssuu}}
                <div class="car2">
                <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                    <iframe src={{firstIssuu}} title="Issuu Preview" width="500" height="375"></iframe>
                    </div>
                    {{#issuesWithLinks}}
                    <div class="carousel-item">
                    <iframe src={{issuuLink}} title="Issuu preview" width="500" height="375"></iframe>
                    </div>
                    {{/issuesWithLinks}}
                    {{!-- <div class="carousel-item">
                    <img class="d-block w-100" src="/images/red.png" alt="Second slide">
                    </div>
                    <div class="carousel-item">
                    <iframe src="https://issuu.com/thebrownnoser/docs/april_2018_noser" title="Issuu preview" width="500" height="375"></iframe>
                    </div> --}}
                </div>
                <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
                </div>
                </div>
                </div>
                {{/if}}
        </div>
    </div>
</div>

<!--Footer-->
<div class="footer">
    <div class="footer-text">Â© The Brown Noser</div>
</div>

